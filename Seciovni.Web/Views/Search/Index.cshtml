@{
    ViewData["title"] = "Invoice Search";
}

<h1>Invoice Search</h1>
<p>Enter desired fields to search below.</p>

<form method="post" action="#" id="searchForm" onsubmit="return false">
    <div id="searchTermHeaders" class="row">
        <div class="searchFieldCol cell"><b>Search Field</b></div>
        <div class="searchTermsCol cell"><b>Search Term(s)</b></div>

    </div>
    <div id="searchTerms">
    </div>
    <div id="searchButton"><img src="~/images/search.svg" alt="" /> Search</div>
</form>

<h2>Results</h2>
<div id="results">

</div>

